{
  "name": "@nearform/observer-samples",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "version": "0.0.1",
  "description": "Sample protobuf datasets generated by scripts simulating introspection of a libp2p node",
  "author": "AlanSl <alan.slater@nearform.com>",
  "homepage": "https://github.com/nearform/libp2p-observer#readme",
  "license": "MIT",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/libp2p-observer.git"
  },
  "scripts": {
    "test": "tap | tap-mocha-reporter progress",
    "mock": "node ./mock/mock.js --",
    "mock-file": "node ./mock/mock.js --file",
    "mock-sock": "node ./mock/mock.js --socksrv",
    "prepare": "webpack"
  },
  "bugs": {
    "url": "https://github.com/nearform/libp2p-observer/issues"
  },
  "devDependencies": {
    "@nearform/observer-data": "^0.0.1",
    "@nearform/observer-proto": "^0.0.1",
    "tap": "^14.9.2",
    "tap-mocha-reporter": "^5.0.0"
  },
  "dependencies": {
    "ws": "^7.2.1"
  },
  "jest": {
    "transform": {
      "\\.(mock|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__jest__/filePaths.js"
    }
  }
}
