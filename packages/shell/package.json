  "author": "AlanSl <alan.slater@nearform.com>",
  "homepage": "https://github.com/nearform/libp2p-introspection-ui#readme",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./index.js",
  "repository": {
    "type": "git",
