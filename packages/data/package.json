{
  "name": "@libp2p/observer-data",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "version": "0.0.1",
  "description": "Javascript protobuf decoder for libp2p-introspection messages, with helper functions",
  "author": "AlanSl <alan.slater@nearform.com>",
  "homepage": "https://github.com/nearform/libp2p-observer#readme",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/libp2p-observer.git"
  },
  "scripts": {
    "test": "CI=true jest"
  },
  "bugs": {
    "url": "https://github.com/nearform/libp2p-observer/issues"
  },
  "dependencies": {
    "@libp2p/observer-proto": "^0.0.1"
  },
  "devDependencies": {
    "@libp2p/observer-samples": "^0.0.1",
    "jest": "^24.9.0",
    "jest-expect-message": "^1.0.2",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-expect-message",
      "<rootDir>/../../__jest__/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/../../__jest__/styleMock.js"
    },
    "transform": {
      "\\.(mock|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__jest__/filePaths.js",
      "\\.js$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    }
  }
}
