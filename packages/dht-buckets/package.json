{
  "name": "@libp2p/observer-dht-buckets",
  "description": "Widget built on @libp2p/observer-sdk, visualising active and recent peers used in DHT queries",
  "author": "AlanSl <alan.slater@nearform.com>",
  "main": "./build/index.js",
  "module": "./index.js",
  "homepage": "",
  "license": "MIT",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/libp2p-observer.git"
  },
  "scripts": {
    "prepare": "webpack",
    "storybook": "start-storybook -p 9009",
    "test": "CI=true jest",
    "test:watch": "jest --watch",
    "update-snapshots": "npm run test -- -u"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@libp2p/observer-shell": "^1.0.0",
    "@storybook/react": "^5.2.4",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "raw-loader": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "storybook": "^5.1.11"
  },
  "peerDependencies": {
    "@libp2p/observer-data": "^0.0.2",
    "@libp2p/observer-sdk": "^0.0.2",
    "react": "*",
    "styled-components": "*"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/../../__jest__/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/../../__jest__/styleMock.js"
    },
    "transform": {
      "\\.(mock|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__jest__/filePaths.js",
      "\\.js$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    }
  },
  "gitHead": "9a1f73f71e98b6e8f27ac26133c7fc63e1fc8f7d"
}
