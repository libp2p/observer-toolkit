{
  "name": "@libp2p-observer/catalogue",
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "version": "0.0.1",
  "description": "React web app containing a browsable catalogue of ui components visualizing libp2p introspection data",
  "author": "AlanSl <alan.slater@nearform.com>",
  "homepage": "https://nearform.github.io/libp2p-introspection-ui",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "CI=true craco test",
    "test:watch": "craco test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "peerDependencies": {
    "@libp2p-observer/data": "^0.0.1",
    "@libp2p-observer/sdk": "^0.0.1"
  },
  "dependencies": {
    "@baristalabs/craco-raw-loader": "^1.1.1",
    "@craco/craco": "5.5.0",
    "@libp2p-observer/connections-table": "^0.0.1",
    "@libp2p-observer/dht-buckets": "^0.0.1",
    "@libp2p-observer/events-table": "^0.0.1",
    "@libp2p-observer/samples": "^0.0.1",
    "@libp2p-observer/shell": "^0.0.1",
    "@libp2p-observer/streams-table": "^0.0.1",
    "arraybuffer-loader": "^1.0.8",
    "craco-babel-loader": "0.1.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-markdown": "^4.2.2",
    "styled-components": "^4.4.0",
    "webpack": "4.41.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "create-react-app": "3.2.0",
    "gh-pages": "^2.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "\\.(mock|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__jest__/filePaths.js",
      "\\.js$": [ "babel-jest", { "rootMode": "upward" } ]
    }
  }

}
